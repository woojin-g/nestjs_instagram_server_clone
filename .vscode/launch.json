{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug NestJS",
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "start:debug"
      ],
      "console": "integratedTerminal",
      "restart": true,
      "sourceMaps": true,
      "envFile": "${workspaceFolder}/.env",
      "autoAttachChildProcesses": true
    }
  ]
}